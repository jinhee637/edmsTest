<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings enhancementEnabled="true" useStatementNamespaces="true" />
	<!-- 객체와 DB table을 맵핑하기 위한 xml파일의 위치를 지정하는 부분이다. -->
	<sqlMap resource="sqlmap/sql/com/Index.xml" />
	<sqlMap resource="sqlmap/sql/com/File.xml" />
	<sqlMap resource="sqlmap/sql/admin/Dept.xml" />
	<sqlMap resource="sqlmap/sql/admin/User.xml" />
	<sqlMap resource="sqlmap/sql/admin/Doc.xml" />
	<sqlMap resource="sqlmap/sql/admin/Category.xml" />
	<sqlMap resource="sqlmap/sql/user/Doc.xml" />
	<sqlMap resource="sqlmap/sql/user/MyDoc.xml" />
	<sqlMap resource="sqlmap/sql/user/User.xml" />
</sqlMapConfig>